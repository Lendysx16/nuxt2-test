  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/svg": "^0.4.1",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@nuxtjs/vercel-builder": "^0.25.0",
    "babel-eslint": "^10.1.0",
